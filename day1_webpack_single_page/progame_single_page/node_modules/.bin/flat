#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/alex/Documents/Formations/theHackingProject4/week4/day1_/progame_single_page/node_modules/flat/node_modules:/Users/alex/Documents/Formations/theHackingProject4/week4/day1_/progame_single_page/node_modules:/Users/alex/Documents/Formations/theHackingProject4/week4/day1_/node_modules:/Users/alex/Documents/Formations/theHackingProject4/week4/node_modules:/Users/alex/Documents/Formations/theHackingProject4/node_modules:/Users/alex/Documents/Formations/node_modules:/Users/alex/Documents/node_modules:/Users/alex/node_modules:/Users/node_modules:/node_modules:/Users/alex/Documents/Formations/theHackingProject4/week4/day1_/progame_single_page/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/alex/Documents/Formations/theHackingProject4/week4/day1_/progame_single_page/node_modules/flat/node_modules:/Users/alex/Documents/Formations/theHackingProject4/week4/day1_/progame_single_page/node_modules:/Users/alex/Documents/Formations/theHackingProject4/week4/day1_/node_modules:/Users/alex/Documents/Formations/theHackingProject4/week4/node_modules:/Users/alex/Documents/Formations/theHackingProject4/node_modules:/Users/alex/Documents/Formations/node_modules:/Users/alex/Documents/node_modules:/Users/alex/node_modules:/Users/node_modules:/node_modules:/Users/alex/Documents/Formations/theHackingProject4/week4/day1_/progame_single_page/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../flat/cli.js" "$@"
else
  exec node  "$basedir/../flat/cli.js" "$@"
fi
