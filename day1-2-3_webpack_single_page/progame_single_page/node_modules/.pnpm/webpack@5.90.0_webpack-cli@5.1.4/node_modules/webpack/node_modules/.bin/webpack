#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/alex/Documents/Formations/theHackingProject4/week3/day5_webpack_and_parcel_starter/webpack_starter/node_modules/.pnpm/webpack@5.90.0_webpack-cli@5.1.4/node_modules/webpack/bin/node_modules:/Users/alex/Documents/Formations/theHackingProject4/week3/day5_webpack_and_parcel_starter/webpack_starter/node_modules/.pnpm/webpack@5.90.0_webpack-cli@5.1.4/node_modules/webpack/node_modules:/Users/alex/Documents/Formations/theHackingProject4/week3/day5_webpack_and_parcel_starter/webpack_starter/node_modules/.pnpm/webpack@5.90.0_webpack-cli@5.1.4/node_modules:/Users/alex/Documents/Formations/theHackingProject4/week3/day5_webpack_and_parcel_starter/webpack_starter/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/alex/Documents/Formations/theHackingProject4/week3/day5_webpack_and_parcel_starter/webpack_starter/node_modules/.pnpm/webpack@5.90.0_webpack-cli@5.1.4/node_modules/webpack/bin/node_modules:/Users/alex/Documents/Formations/theHackingProject4/week3/day5_webpack_and_parcel_starter/webpack_starter/node_modules/.pnpm/webpack@5.90.0_webpack-cli@5.1.4/node_modules/webpack/node_modules:/Users/alex/Documents/Formations/theHackingProject4/week3/day5_webpack_and_parcel_starter/webpack_starter/node_modules/.pnpm/webpack@5.90.0_webpack-cli@5.1.4/node_modules:/Users/alex/Documents/Formations/theHackingProject4/week3/day5_webpack_and_parcel_starter/webpack_starter/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/webpack.js" "$@"
else
  exec node  "$basedir/../../bin/webpack.js" "$@"
fi
